<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:nfs="http://www.nfs.com/schema/nfs"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.nfs.com/schema/nfs http://www.nfs.com/schema/nfs/nfs.xsd">
    <bean id="helloWorld" class="code.google.nfs.rpc.config.reflection.HelloWorldComponent"/>

    <nfs:service targetInstanceName="helloServiceImpl1"
                 interfaceName="code.google.nfs.rpc.config.reflection.HelloWorldService" ref="helloWorld"/>
    <nfs:registry port="18888" id="re"/>


</beans>